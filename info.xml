<?xml version="1.0"?>
<extension key="de.systopia.amazonbounceapi" type="module">
  <file>amazonbounceapi</file>
  <name>Amazon SES Bounce API</name>
  <description>Handling parsed Amazon SES bounce notifications, analogue to https://github.com/mecachisenros/civicrm-ses. This is best used with CiviProxy webhook2api, or something similar </description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Philipp Batroff</author>
    <email>batroff@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.amazonbounceapi</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.amazonbounceapi</url>
    <url desc="Support">https://github.com/systopia/de.systopia.amazonbounceapi</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-02-16</releaseDate>
  <version>0.2</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Creates an API to handle amazon bounces. Best used with CiviProxy webhook2api module.</comments>
  <civix>
    <namespace>CRM/Amazonbounceapi</namespace>
    <format>22.10.0</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
</extension>
