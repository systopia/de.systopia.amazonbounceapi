  <license>AGPL-3.0</license>
  <maintainer>
    <author>Philipp Batroff</author>
    <email>batroff@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.amazonbounceapi</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.amazonbounceapi</url>
    <url desc="Support">https://github.com/systopia/de.systopia.amazonbounceapi</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-01-15</releaseDate>
  <version>1.0</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <civix>
    <namespace>CRM/Amazonbounceapi</namespace>
  </civix>
</extension>
